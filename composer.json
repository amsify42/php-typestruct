{
    "name": "amsify42/php-typestruct",
    "description": "PHP package for validating data against the structure defined",
    "keywords": ["typestruct", "php typestruct", "php validation", "validation"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohd Samiullah",
            "email": "amsify42@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0.0"
    },
    "require-dev": {
        "phpunit/phpunit":  "^7.0"
    },
    "autoload": {
        "files": [
            "src/Helper/global.php"
        ],
        "psr-4": {
            "Amsify42\\TypeStruct\\": "src/"
        }
    },
    "autoload-dev": {
        "files": [
            "tests/global.php"
        ],
        "psr-4": {
            "Amsify42\\Tests\\": "tests/"
        }
    }
}
